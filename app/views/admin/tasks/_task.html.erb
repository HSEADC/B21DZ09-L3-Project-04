<div id="<%= dom_id task %>">
  <div class="taskCard">
    <%= image_tag task.first_answer_image.url, width: 300 if task.first_answer_image.url %>
    <%= image_tag task.ref_image.url, width: 300 if task.ref_image.url %>

    <p>
      <strong><%= task.name %></strong>
    </p>

    <p>
      <%= task.description %>
    </p>

  </div>
</div>
